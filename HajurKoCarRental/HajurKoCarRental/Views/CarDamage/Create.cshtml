@model HajurKoCarRental.Models.CarDamage

@{
	ViewData["Title"] = "Create";
}


<section class="bg0 p-t-23 p-b-130 mt-5">
	<h1 class=" ltext-103 cl5 text-dark">Create Damage Car Information</h1>
	<hr class=" text-dark " style="height:3px; width:100%; border-width:0;" />
	<div class="row">
		<div class="col-md-4">
			<form asp-action="Create">
				<div asp-validation-summary="ModelOnly" class="text-danger"></div>
				<div class="form-group">
					<label asp-for="description" class="control-label text-dark"></label>
					<input asp-for="description" class="form-control" />
					<span asp-validation-for="description" class="text-danger"></span>
				</div>
				<div class="form-group">
					<label asp-for="RentalID" class="control-label text-dark"></label>
					<select asp-for="RentalID" class="form-control" asp-items="@(new SelectList(ViewBag.RentalID, "Value", "Text"))"></select>
				</div>

				<div class="form-group">
					<input type="submit" value="Create" class="btn btn-dark" />
					<a asp-action="Index" class="btn btn-dark">Cancel</a>
				</div>
			</form>
		</div>
	</div>

</section>
@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}
